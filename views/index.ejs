<%- include('./partials/header.ejs') %>
    <h1>The Music Box</h1>
    <ul>
        <% for(let i = 0; i < music.length; i++) { %>
          <li>
            <a href="/music/<%= music[i].id %>"> <%= music[i].title %> </a>
            <form action="/music/<%=music[i].id%>?_method=DELETE" method="POST">
              <input type="submit" value="DELETE" />  
            </form>
           </li>
        <% } %>
      </ul>
